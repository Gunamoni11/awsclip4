node('main'){
    stage('Launch configuration'){
        def cmd = "aws autoscaling create-launch-configuration --launch-configuration-name mylc5 --image-id ami-0fb653ca2d3203ac1 --instance-type t2.micro --security-groups sg-0244e3f85210cc582 --key-name terra --user-data file://userdata.txt --region us-east-2"
        sh (cmd)
    }
    stage('Auto scaling'){
        sh "aws autoscaling create-auto-scaling-group --auto-scaling-group-name myasg5 --launch-configuration-name mylc4 --max-size 2 --min-size 1 --availability-zones us-east-2c --region us-east-2"
    }
}
